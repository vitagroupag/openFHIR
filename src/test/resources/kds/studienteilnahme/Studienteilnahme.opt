                <archetype_id>
                    <value>openEHR-EHR-ACTION.informed_consent.v0</value>
                </archetype_id>
                <term_definitions code="at0000">
                    <items id="text">Einwilligungserklrung</items>
                    <items id="description">Aufzeichnungen ber den Status und Einzelheiten der Einwilligungserklrung eines Patienten (oder seines Vertreters) zur einen vorgeschlagenen Prozedur, Studie oder einen anderen gesundheitsbezogenen Aktivitt (einschlielich Behandlungen und Untersuchungen) auf der Grundlage einer klaren Einschtzung und eines klaren Verstndnisses der Fakten, Auswirkungen und mglichen zuknftigen Konsequenzen durch den Patienten oder dessen Vertreter.</items>
                </term_definitions>
                <term_definitions code="at0001">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0002">
                    <items id="text">Prozedur / Studie / Aktivitt</items>
                    <items id="description">Identifizierung der Prozedur, klinischen Studie oder der gesundheitsbezogenen Aktivitt (einschlielich der richtigen Stelle / des richtigen Standorts, falls zutreffend), fr die der Einwilligungsstatus und Details aufgezeichnet werden.</items>
                </term_definitions>
                <term_definitions code="at0003">
                    <items id="text">Einzelheiten</items>
                    <items id="description">Zustzliche strukturierte Details zur Prozedur / Studie / zur Aktivitt selbst oder zustzliche strukturierte Details der Einwilligung.</items>
                    <items id="comment">Zum Beispiel: Absicht / Zweck; ansthetische Details; Offenlegung wesentlicher Risiken; und diskutierte vorgeschlagene Vorteile.</items>
                </term_definitions>
                <term_definitions code="at0004">
                    <items id="text">Form der Einwilligung</items>
                    <items id="description">Form der beantragten oder erteilten Einwilligung.</items>
                </term_definitions>
                <term_definitions code="at0005">
                    <items id="text">Schriftlich</items>
                    <items id="description">Das Format der Einwilligung ist schriftlich.</items>
                </term_definitions>
                <term_definitions code="at0006">
                    <items id="text">Mndlich</items>
                    <items id="description">Das Format der Einwilligung ist mndlich.</items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="text">Grund</items>
                    <items id="description">Grund, warum der Verlaufsschritt fr die benannte Prozedur, klinische Studie oder die gesundheitsbezogene Aktivitt durchgefhrt wurde.</items>
                    <items id="comment">Zum Beispiel der Grund fr "Einwilligung verweigert" oder "Einwilligung widerrufen".</items>
                </term_definitions>
                <term_definitions code="at0008">
                    <items id="text">Beginn</items>
                    <items id="description">Datum und optionale Uhrzeit, zu der die Gltigkeit der Einwilligungserklrung wirksam wird.</items>
                </term_definitions>
                <term_definitions code="at0009">
                    <items id="text">Endtermin</items>
                    <items id="description">Datum und optionale Uhrzeit, zu der die Gltigkeit der Einwilligungserklrung abgelaufen ist.</items>
                </term_definitions>
                <term_definitions code="at0010">
                    <items id="text">Vorbehalt</items>
                    <items id="description">Einzelheiten zu Bedingungen oder Ausnahmen der Einwilligungserklrung.</items>
                </term_definitions>
                <term_definitions code="at0011">
                    <items id="text">Beschreibung der Einwilligung</items>
                    <items id="description">Beschreibung der Einwilligungserklrung, die erforderlich ist oder aufgezeichnet werden soll, bevor die vorgeschlagene Prozedur, klinische Prfung oder gesundheitsbezogene Aktivitten durchgefhrt werden.</items>
                </term_definitions>
                <term_definitions code="at0012">
                    <items id="text">Einwilligungsnachweis</items>
                    <items id="description">Link zum Einwilligungsnachweis.</items>
                    <items id="comment">Beispielsweise wird eine Audio-Einwilligungserklrung angefordert oder ein Bild der schriftlichen Einwilligung erhalten.</items>
                </term_definitions>
                <term_definitions code="at0013">
                    <items id="text">Geplant</items>
                    <items id="description">Die Notwendigkeit einer Einwilligungserklrung wird festgestellt.</items>
                </term_definitions>
                <term_definitions code="at0014">
                    <items id="text">Einwilligung erbeten</items>
                    <items id="description">Die Einwilligungserklrung wurde vom Patienten oder seinem Vertreter angefragt, es ist jedoch keine Antwort eingegangen.</items>
                </term_definitions>
                <term_definitions code="at0015">
                    <items id="text">Einwilligung erteilt</items>
                    <items id="description">Die Einwilligungserklrung wurde vom Patienten oder seinem Vertreter erteilt.</items>
                </term_definitions>
                <term_definitions code="at0016">
                    <items id="text">Einwilligung verweigert</items>
                    <items id="description">Als Antwort auf eine Anfrage zur Einwilligungserklrung, wurde sie vom Patienten oder seinem Vertreter abgelehnt.</items>
                </term_definitions>
                <term_definitions code="at0017">
                    <items id="text">Einwilligung widerrufen</items>
                    <items id="description">Nach der ersten bermittlung einer Einwilligungserklrung wurde diese vom Patienten oder seinem Vertreter widerrufen.</items>
                </term_definitions>
                <term_definitions code="at0018">
                    <items id="text">Abgesagt</items>
                    <items id="description">Die Absicht, eine Einwilligungserklrung anzufordern, wurde aufgehoben, bevor die Einwilligung des Patienten oder seines Vertreters angefordert wurde.</items>
                </term_definitions>
                <term_definitions code="at0019">
                    <items id="text">Verschoben</items>
                    <items id="description">Die Absicht, eine Einwilligungserklrung anzufordern, wurde verschoben.</items>
                </term_definitions>
                <term_definitions code="at0021">
                    <items id="text">Einwilligung nicht erhalten</items>
                    <items id="description">Eine Einwilligungserklrung war weder vom Patienten noch vom Vertreter des Patienten erhltlich.</items>
                </term_definitions>
                <term_definitions code="at0022">
                    <items id="text">Abgeschlossen</items>
                    <items id="description">Der Patient oder sein Vertreter hat eine Einwilligungserklrung abgegeben und die Aktivitt ist nun abgeschlossen.</items>
                </term_definitions>
                <term_definitions code="at0024">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0025">
                    <items id="text">Verwendetes Einwilligungsdokument</items>
                    <items id="description">Identifizierung des verwendeten Einwilligungsformulars oder Dokuments.</items>
                </term_definitions>
                <term_definitions code="at0026">
                    <items id="text">Prfungsdatum</items>
                    <items id="description">Datum, an dem der Einwilligungsstatus berprft werden muss.</items>
                </term_definitions>
                <term_definitions code="at0027">
                    <items id="text">Termin geplant</items>
                    <items id="description">Es wurde ein Termin vereinbart, um die Zustimmung einzuholen.</items>
                </term_definitions>
                <term_definitions code="at0028">
                    <items id="text">Anforderer der Einwilligungserklrung</items>
                    <items id="description">Angaben zum Gesundheitsdienstleister, der die Einwilligung anfordert oder aufzeichnet.</items>
                    <items id="comment">Fgen Sie archetypisierte demografische Details in diesen SLOT ein, wenn es nicht mglich ist, other_participations zu verwenden.</items>
                </term_definitions>
                <term_definitions code="at0029">
                    <items id="text">Patient/Einwilligende Person</items>
                    <items id="description">Details zu der Person (oder dem Vertreter der Person), die um die Einwilligung zur Prozedur, klinischen Prfung oder gesundheitsbezogenen Aktivitt gebeten wird oder diese erteilt.</items>
                    <items id="comment">Fgen Sie archetypisierte demografische Details in diesen SLOT ein, wenn es nicht mglich ist, other_participations zu verwenden.</items>
                </term_definitions>
                <term_definitions code="at0030">
                    <items id="text">Beschreibung der Prozedur / Studie / Aktivitt</items>
                    <items id="description">Beschreibung der Prozedur, klinischen Prfung oder der gesundheitsbezogenen Aktivitt.</items>
                </term_definitions>
                <term_definitions code="at0031">
                    <items id="text">Absicht</items>
                    <items id="description">Beschreibung der Absicht der Prozedur, klinischen Prfung oder der gesundheitsbezogenen Aktivitt.</items>
                </term_definitions>
                <term_definitions code="at0032">
                    <items id="text">Name</items>
                    <items id="description">Identifizierung der zur Verfgung gestellten Information.</items>
                    <items id="comment">Zum Beispiel der Name des Formulars.</items>
                </term_definitions>
                <term_definitions code="at0033">
                    <items id="text">Patienteninformation</items>
                    <items id="description">Details zur Patienteninformation, die der Person oder dem Vertreter der Person zur Verfgung gestellt werden.</items>
                </term_definitions>
                <term_definitions code="at0035">
                    <items id="text">Beschreibung</items>
                    <items id="description">Beschreibung der zur Verfgung gestellten Patienteninformation.</items>
                </term_definitions>
                <term_definitions code="at0036">
                    <items id="text">Kommentar</items>
                    <items id="description">Zustzliche Beschreibung der Prozedur der Einwilligungserklrung, die in anderen Bereichen nicht erfasst wurde.</items>
                </term_definitions>
                <term_definitions code="at0037">
                    <items id="text">Angaben zum Einwilligungsnachweis</items>
                    <items id="description">Digitale Darstellung des Einwilligungsnachweises.</items>
                    <items id="comment">Zum Beispiel: ein PDF eines Einwilligungsformulars oder eine Audiodatei.</items>
                </term_definitions>
                <term_definitions code="at0038">
                    <items id="text">Multimedia-Darstellung</items>
                    <items id="description">Multimediale Darstellung des Einwilligungsnachweises.</items>
                </term_definitions>
            </children>
            <cardinality>
                <is_ordered>false</is_ordered>
                <is_unique>false</is_unique>
                <interval>
                    <lower_included>true</lower_included>
                    <upper_included>false</upper_included>
                    <lower_unbounded>false</lower_unbounded>
                    <upper_unbounded>true</upper_unbounded>
                    <lower>1</lower>
                </interval>
            </cardinality>
        </attributes>
