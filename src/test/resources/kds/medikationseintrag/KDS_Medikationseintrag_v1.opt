                        <archetype_id>
                            <value>openEHR-EHR-CLUSTER.timing_nondaily.v1</value>
                        </archetype_id>
                        <term_definitions code="at0000">
                            <items id="text">Nicht tägliche Dosierung</items>
                            <items id="description">Strukturierte Informationen über das geplante Zeitablaufmuster für eine therapeutische oder diagnostische Aktivität, die sich über Tage, Wochen, Monate oder Jahre erstreckt.</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">Zeitpunkt</items>
                            <items id="description">Die Aktivität sollte an einem bestimmten Datum oder in einem bestimmten Datumsbereich stattfinden.</items>
                            <items id="comment">Zum Beispiel: "am 12. Januar 2017" oder "vom 30. Oktober 2017 bis 6. November 2017"</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">Periode</items>
                            <items id="description">Das Intervall zwischen den Wiederholungen der Aktivität.</items>
                            <items id="comment">Zum Beispiel: "Alle 3 Wochen". Falls erforderlich, kann mit diesem Element explizit angegeben werden, dass eine Aktivität jeden einzelnen Tag stattfinden soll, indem es auf "1 Tag" gesetzt wird.</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">Bestimmter Wochentag</items>
                            <items id="description">Die Aktivität sollte an einem bestimmten Wochentag stattfinden.</items>
                            <items id="comment">Zum Beispiel: "Am Montag, Mittwoch und Freitag".</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                            <items id="text">Bestimmter Tag des Monats</items>
                            <items id="description">Die Aktivität sollte an einem bestimmten Tag im Monat oder in einem Intervall von mehreren Tagen in dem Monat stattfinden.</items>
                            <items id="comment">Zum Beispiel: "am 3., 13. und 23. jedes Monats" oder "vom 1. bis zum 10. jedes Monats"</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="text">Ereignis</items>
                            <items id="description">Der Name des Ereignisses, das die Aktivität auslöst.</items>
                            <items id="comment">Dieses Element ist für Ereignisse gedacht, die zu verschiedenen Zeitpunkten auftreten können, wie z.B. Beginn der Menstruation, und nicht für Dosis oder Aktivitäten, die von einer anderen Variable abhängen. Bei Bedarf kann der Name des Ereignisses mit einer Terminologie kodiert werden, mit der die Anwendung ein bestimmtes Datum für die Aktivität festlegen kann.</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">Bestimmtes Ereignis</items>
                            <items id="description">Ein bestimmtes, benanntes Zeitereignis, auf das sich die Aktivität bezieht und stattfinden soll.</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="text">Montag</items>
                            <items id="description">Die Aktivität sollte am Montag stattfinden.</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                            <items id="text">Dienstag</items>
                            <items id="description">Die Aktivität sollte am Dienstag stattfinden.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Offset</items>
                            <items id="description">Das Zeitintervall vor oder nach dem Ereignis, in dem die Aktivität stattfinden sollte. Negative Intervalle können verwendet werden, damit signalisiert werden kann, dass die Aktivität vor dem genannten Ereignis stattfinden soll.</items>
                            <items id="comment">Zum Beispiel: "3 Tage nach Beginn der Menstruation = Beginn der Menstruation + 3 Tage", "2 Wochen vor der Aufnahme = Aufnahme - 2 Wochen".</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="text">Ein/Aus-Zyklus</items>
                            <items id="description">Ein Aktivitätszyklus, bei dem ein Ein/Aus-Zyklus erforderlich ist.</items>
                            <items id="comment">Zum Beispiel: "1 Woche nehmen, 2 Wochen auslassen, 4 Mal wiederholen".</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="text">Ein</items>
                            <items id="description">Der Zeitraum, in dem die Aktivität ausgeführt werden soll.</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="text">Aus</items>
                            <items id="description">Der Zeitraum, in dem die Aktivität NICHT stattfinden soll.</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="text">Wiederholungen</items>
                            <items id="description">Die Häufigkeit, mit der der Ein/Aus-Zyklus wiederholt werden soll.</items>
                        </term_definitions>
                        <term_definitions code="at0014">
                            <items id="text">Frequenz</items>
                            <items id="description">Die Anzahl der Tage pro Zeitintervall, an denen die Aktivität stattfinden soll.</items>
                            <items id="comment">Zum Beispiel: "3 mal pro Woche", "2-4 mal pro Monat".</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                            <items id="text">Mittwoch</items>
                            <items id="description">Die Aktitvität sollte am Mittwoch stattfinden.</items>
                        </term_definitions>
                        <term_definitions code="at0017">
                            <items id="text">Donnerstag</items>
                            <items id="description">Die Aktivität sollte am Donnerstag stattfinden.</items>
                        </term_definitions>
                        <term_definitions code="at0018">
                            <items id="text">Freitag</items>
                            <items id="description">Die Aktivität sollte am Freitag stattfinden.</items>
                        </term_definitions>
                        <term_definitions code="at0019">
                            <items id="text">Samstag</items>
                            <items id="description">Die Aktivität sollte am Samstag stattfinden.</items>
                        </term_definitions>
                        <term_definitions code="at0020">
                            <items id="text">Sonntag</items>
                            <items id="description">Die Aktivität sollte am Sonntag stattfinden.</items>
                        </term_definitions>
                        <term_definitions code="at0021">
                            <items id="text">Beschreibung des Zeitablaufs</items>
                            <items id="description">Freitextbeschreibung des Zeitablaufs.</items>
                            <items id="comment">Zum Beispiel: "Eine Woche verwenden, dann zwei Wochen pausieren und dann wiederholen". Dieses Element soll es ermöglichen, die Strukturen für tägliche Zeitabläufe zu verwenden, ohne notwendigerweise die Struktur der nicht-täglichen Zeitabläufe angeben zu müssen.</items>
                        </term_definitions>
                    </children>
