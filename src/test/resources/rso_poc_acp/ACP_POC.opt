                </term_definitions>
                <term_definitions code="OID: 2.16.840.1.113883.2.4.3.11.60.40.2.7.15.1::LW">
                    <items id="text">Levenswensverklaring</items>
                </term_definitions>
                <term_definitions code="OID: 2.16.840.1.113883.2.4.3.11.60.40.2.7.15.1::DO">
                    <items id="text">Verklaring donorschap</items>
                </term_definitions>
                <term_definitions code="OID: 2.16.840.1.113883.2.4.3.11.60.40.2.7.15.1::OTH">
                    <items id="text">Anders</items>
                </term_definitions>
