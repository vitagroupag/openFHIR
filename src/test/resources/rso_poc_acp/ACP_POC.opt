                <term_definitions code="OID: 2.16.840.1.113883.2.4.3.11.60.40.2.7.15.1::VERBVL">
                    <items id="text">Behandelverbod met aanvulling Voltooid Leven</items>
                </term_definitions>
                <term_definitions code="OID: 2.16.840.1.113883.2.4.3.11.60.40.2.7.15.1::MON">
                    <items id="text">Mondelinge afspraak</items>
